{
  "name": "RNS Resolver",
  "description": "RSK Testnet - RNS ContentHash Resolver",
  "manifest_version": 2,
  "version": "0.2.0",
  "options_page": "options.html",
  "background": {
    "page": "background.html"
  },
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": "icon-128.png"
  },
  "icons": {
    "128": "icon-128.png"
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "tabs",
    "*://*.rsk/*",
    "storage"
  ],
  "web_accessible_resources": [
    "loading.html", "invalid.html"
  ]
}
