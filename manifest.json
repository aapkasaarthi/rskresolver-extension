{
    "name": "RNS Resolver",
    "description": "RSK Testnet - RNS ContentHash Resolver",
    "version": "0.1.0",
    "manifest_version": 2,
    "icons": {
        "16": "1.png",
        "32": "2.png",
        "48": "3.png",
        "128": "4.png"
    },
    "background": {
        "scripts": ["./background.js"]
    },
    "browser_action": {
        "default_popup": "popup.html"
    },
    "permissions": [
        "webRequest",
        "webRequestBlocking",
        "tabs",
        "*://*.rsk/*"
    ]
}
